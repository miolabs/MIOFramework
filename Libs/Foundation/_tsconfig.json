{
  "compilerOptions": {
    "target": "es5",    
    "sourceMap": true,
    "declaration": true,
    "outDir": ".build",
    "rootDir": "source",
    "strict": false,
    "noImplicitThis": false,
    "removeComments": true,
    "noImplicitAny": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "typeRoots": [
	  "node_modules/@types/"    
  ]
  },
  "exclude": [
	  "node_modules/"
  ]
}
